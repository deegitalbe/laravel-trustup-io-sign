{
    "name": "deegitalebe/package-sign",
    "description": "Sign url builder.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Pierre Wasilewski, Mathieu Henrotay",
            "email": "pierre.wasilewski@trustup.be"
        }
    ],
    "require": {
        "henrotaym/laravel-package-versioning-config": "^1.3",
        "deegitalbe/laravel-trustup-io-external-model-relations": "^1.1"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
